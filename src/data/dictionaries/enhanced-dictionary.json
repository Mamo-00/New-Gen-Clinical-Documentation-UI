{
  "mikroskopiPanels": [
    {
      "id": "generalArchitecture",
      "title": "Generelle Arkitektoniske Funn",
      "fields": [
        {
          "id": "lesionType",
          "label": "Lesjonstype (Overordnet)",
          "type": "dropdown",
          "options": [
            "Polypoid",
            "Flat",
            "Ulcererende",
            "Infiltrativ",
            "Ingen distinkt lesjon"
          ],
          "defaultValue": "Ingen distinkt lesjon",
          "generatesText": "Lesjonen fremstår som {{value}}."
        },
        {
          "id": "glandularPattern",
          "label": "Kjertelmønster",
          "type": "dropdown",
          "options": [
            "Bevarte krypter",
            "Tubulært",
            "Villøst",
            "Tubulovilløst",
            "Kribriformt",
            "Papillært",
            "Mikropapillært",
            "Solid",
            "Trabekulært",
            "Acinært",
            "Ingen kjertler/Udifferensiert"
          ],
          "defaultValue": "Bevarte krypter",
          "generatesText": "Kjertelmønsteret er overveiende {{value}}."
        },
        {
          "id": "epithelialSurface",
          "label": "Overflateepitel",
          "type": "dropdown",
          "options": [
            "Intakt",
            "Erodert",
            "Ulcerert",
            "Dekket av fibrinopurulent materiale"
          ],
          "defaultValue": "Intakt",
          "generatesText": "Overflateepitelet er {{value}}."
        },
        {
          "id": "stromalReaction",
          "label": "Stromareaksjon",
          "type": "dropdown",
          "options": [
            "Ingen signifikant",
            "Desmoplastisk",
            "Myksoid",
            "Fibrotisk",
            "Ødematøs",
            "Inflammatorisk infiltrat"
          ],
          "defaultValue": "Ingen signifikant",
          "generatesText": "Det sees {{value}} stromal reaksjon."
        },
        {
          "id": "necrosisPresent",
          "label": "Nekrose",
          "type": "checkbox",
          "defaultValue": false,
          "generatesText": "{{value ? Nekrose er til stede. : Ingen tydelig nekrose observeres.}}"
        },
        {
          "id": "necrosisType",
          "label": "Type Nekrose",
          "type": "dropdown",
          "options": [
            "Koagulasjonsnekrose",
            "Komedonekrose",
            "Tumornekrose (uspesifikk)"
          ],
          "defaultValue": "Tumornekrose (uspesifikk)",
          "conditionalOn": "necrosisPresent",
          "conditionalValue": true,
          "generatesText": "Nekrosen er av typen {{value}}."
        }
      ]
    },

    {
      "id": "cellularFeatures",
      "title": "Cellulære Funn",
      "fields": [
        {
          "id": "cellTypeDominant",
          "label": "Dominerende Celletype",
          "type": "dropdown",
          "options": [
            "Epiteliale celler",
            "Spindelceller",
            "Rundceller (lymfoide)",
            "Uspesifikke/Pleomorfe"
          ],
          "defaultValue": "Epiteliale celler",
          "generatesText": "De dominerende cellene er {{value}}."
        },
        {
          "id": "nuclearAtypiaGrade",
          "label": "Kjerneatypi (grad)",
          "type": "dropdown",
          "options": ["Ingen/Minimal", "Lett", "Moderat", "Markert/Alvorlig"],
          "defaultValue": "Ingen/Minimal",
          "generatesText": "Kjerneatypien vurderes som {{value}}."
        },
        {
          "id": "nuclearFeatures",
          "label": "Kjernekarakteristika",
          "type": "checkboxGroup",
          "options": [
            "Hyperkromasi",
            "Pleomorfi",
            "Vesikulært kromatin",
            "Uregelmessig kjernemembran"
          ],
          "generatesText": "Kjernene viser {{value}}.",
          "conditionalOn": "nuclearAtypiaGrade",
          "conditionalValue_not": "Ingen/Minimal"
        },
        {
          "id": "nucleoliProminence",
          "label": "Nukleoler",
          "type": "dropdown",
          "options": ["Lite fremtredende", "Fremtredende", "Multiple"],
          "defaultValue": "Lite fremtredende",
          "generatesText": "Nukleolene er {{value}}."
        },
        {
          "id": "cytoplasmAppearance",
          "label": "Cytoplasma",
          "type": "dropdown",
          "options": [
            "Lite/sparsomt",
            "Eosinofilt",
            "Basofilt",
            "Klarcellet",
            "Granulært",
            "Vakuolisert"
          ],
          "defaultValue": "Eosinofilt",
          "generatesText": "Cytoplasma er {{value}}."
        },
        {
          "id": "mitoticActivityObserved",
          "label": "Mitotisk Aktivitet Observert",
          "type": "checkbox",
          "defaultValue": false,
          "generatesText": "{{value ? Mitotisk aktivitet observeres. : Ingen åpenbar mitotisk aktivitet.}}"
        },
        {
          "id": "mitoticCount",
          "label": "Mitosetall (per 10 HPF)",
          "type": "number",
          "unit": "/10 HPF",
          "defaultValue": 0,
          "conditionalOn": "mitoticActivityObserved",
          "conditionalValue": true,
          "generatesText": "{{value > 0 ? value +  mitoser per 10 HPF. : }}"
        },
        {
          "id": "atypicalMitoses",
          "label": "Atypiske Mitoser",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "mitoticActivityObserved",
          "conditionalValue": true,
          "generatesText": "{{value ? Atypiske mitoser er til stede. : }}"
        }
      ]
    },

    {
      "id": "dysplasiaFeatures",
      "title": "Dysplastiske Funn",
      "fields": [
        {
          "id": "dysplasiaPresent",
          "label": "Dysplasi Til Stede",
          "type": "checkbox",
          "defaultValue": false,
          "generatesText": "{{value ? Dysplasi er påvist. : Ingen sikker dysplasi påvist.}}"
        },
        {
          "id": "dysplasiaGrade",
          "label": "Dysplasigrad",
          "type": "dropdown",
          "options": ["Lavgradig", "Høygradig", "Ubestemt for dysplasi"],
          "defaultValue": "Lavgradig",
          "conditionalOn": "dysplasiaPresent",
          "conditionalValue": true,
          "generatesText": "Dysplasien er {{value}}."
        },

        {
          "id": "hgd_complexGlands",
          "label": "Komplekse kjertelformasjoner (HGD)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "dysplasiaGrade",
          "conditionalValue": "Høygradig",
          "generatesText": "{{value ? Det sees komplekse kjertelformasjoner. : }}"
        },
        {
          "id": "hgd_cribriform",
          "label": "Kribriform arkitektur (HGD)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "dysplasiaGrade",
          "conditionalValue": "Høygradig",
          "generatesText": "{{value ? Kribriform arkitektur er til stede. : }}"
        },
        {
          "id": "hgd_micropapillary",
          "label": "Mikropapillære formasjoner (HGD)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "dysplasiaGrade",
          "conditionalValue": "Høygradig",
          "generatesText": "{{value ? Mikropapillære formasjoner observeres. : }}"
        },
        {
          "id": "hgd_intraluminalNecrosis",
          "label": "Intraluminal nekrose (HGD)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "dysplasiaGrade",
          "conditionalValue": "Høygradig",
          "generatesText": "{{value ? Intraluminal nekrose er til stede. : }}"
        },

        {
          "id": "hgd_lossOfPolarity",
          "label": "Tap av kjernepolarisering (HGD)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "dysplasiaGrade",
          "conditionalValue": "Høygradig",
          "generatesText": "{{value ? Det er betydelig tap av kjernepolarisering. : }}"
        },
        {
          "id": "hgd_enlargedVesicularNuclei",
          "label": "Forstørrede kjerner, vesikulært kromatin (HGD)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "dysplasiaGrade",
          "conditionalValue": "Høygradig",
          "generatesText": "{{value ? Kjernene er forstørrede med vesikulært kromatinmønster. : }}"
        },
        {
          "id": "hgd_prominentNucleoli",
          "label": "Prominente nukleoler (HGD)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "dysplasiaGrade",
          "conditionalValue": "Høygradig",
          "generatesText": "{{value ? Nukleolene er prominente. : }}"
        }
      ]
    },

    {
      "id": "serratedFeatures",
      "title": "Sagtakkede Funn",
      "fields": [
        {
          "id": "isSerratedLesionSuspected",
          "label": "Mistanke om Sagtakket Lesjon",
          "type": "checkbox",
          "defaultValue": false,
          "generatesText": ""
        },

        {
          "id": "serrationLocation",
          "label": "Sagtakkingens Plassering",
          "type": "dropdown",
          "options": [
            "Luminalt (øvre 2/3)",
            "Basalt (nedre 1/3)",
            "Gjennomgående"
          ],
          "defaultValue": "Luminalt (øvre 2/3)",
          "conditionalOn": "isSerratedLesionSuspected",
          "conditionalValue": true,
          "generatesText": "Sagtakkingen er primært lokalisert {{value}}."
        },
        {
          "id": "hp_cryptArchitecture",
          "label": "Kryptarkitektur (HP-type)",
          "type": "dropdown",
          "options": [
            "Traktformede krypter (MVHP)",
            "Dilaterte krypter (GCHP)"
          ],
          "defaultValue": "Traktformede krypter (MVHP)",
          "conditionalOn": "isSerratedLesionSuspected",
          "conditionalValue": true,
          "generatesText": "Kryptene er {{value}}."
        },

        {
          "id": "bsl_asymmetricCryptDilation",
          "label": "Asymmetrisk dilatasjon kryptbasis (BSL)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "isSerratedLesionSuspected",
          "conditionalValue": true,
          "generatesText": "{{value ? Asymmetrisk dilatasjon av kryptbasis sees. : }}"
        },
        {
          "id": "bsl_horizontalGrowth",
          "label": "Horisontal vekst langs muscularis mucosae (BSL)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "isSerratedLesionSuspected",
          "conditionalValue": true,
          "generatesText": "{{value ? Horisontal vekst langs muscularis mucosae observeres. : }}"
        },
        {
          "id": "bsl_basalCryptSerration",
          "label": "Sagtakking involverer kryptbasis (BSL)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "isSerratedLesionSuspected",
          "conditionalValue": true,
          "generatesText": "{{value ? Sagtakking involverer kryptbasis. : }}"
        },
        {
          "id": "bsl_expandedProliferationZone",
          "label": "Utvidet proliferasjonssone (BSL)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "isSerratedLesionSuspected",
          "conditionalValue": true,
          "generatesText": "{{value ? Proliferasjonssonen er utvidet. : }}"
        },

        {
          "id": "tsa_slitLikeSerrations",
          "label": "Spalteformede sagtakkinger (TSA)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "isSerratedLesionSuspected",
          "conditionalValue": true,
          "generatesText": "{{value ? Det sees spalteformede sagtakkinger liknende tynntarmsmucosa. : }}"
        },
        {
          "id": "tsa_eosinophilicCytoplasm",
          "label": "Høye sylinderepitelceller, eosinofilt cytoplasma (TSA)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "isSerratedLesionSuspected",
          "conditionalValue": true,
          "generatesText": "{{value ? Cellene er høye sylindriske med tydelig eosinofilt cytoplasma. : }}"
        },
        {
          "id": "tsa_pencilateNuclei",
          "label": "Blyantliknende kjerner (TSA)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "isSerratedLesionSuspected",
          "conditionalValue": true,
          "generatesText": "{{value ? Kjernene er avlange/blyantliknende. : }}"
        },
        {
          "id": "tsa_ectopicCryptFoci",
          "label": "Ektopiske kryptfoci (TSA)",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "isSerratedLesionSuspected",
          "conditionalValue": true,
          "generatesText": "{{value ? Ektopiske kryptfoci er til stede. : }}"
        }
      ]
    },

    {
      "id": "inflammatoryInfiltrate",
      "title": "Inflammatorisk Infiltrat",
      "fields": [
        {
          "id": "inflammationPresent",
          "label": "Inflammasjon Til Stede",
          "type": "checkbox",
          "defaultValue": false,
          "generatesText": "{{value ? Det sees inflammatorisk infiltrat. : Minimalt eller ingen signifikant inflammasjon.}}"
        },
        {
          "id": "inflammationType",
          "label": "Type Inflammasjon",
          "type": "dropdown",
          "options": [
            "Akutt",
            "Kronisk",
            "Blandet akutt og kronisk",
            "Granulomatøs",
            "Eosinofil-rik"
          ],
          "defaultValue": "Kronisk",
          "conditionalOn": "inflammationPresent",
          "conditionalValue": true,
          "generatesText": "Inflammasjonen er overveiende {{value}}."
        },
        {
          "id": "inflammationDensity",
          "label": "Inflammasjonsdensitet",
          "type": "dropdown",
          "options": ["Lett", "Moderat", "Markert"],
          "defaultValue": "Lett",
          "conditionalOn": "inflammationPresent",
          "conditionalValue": true,
          "generatesText": "Densiteten av infiltratet er {{value}}."
        },
        {
          "id": "inflammatoryCellTypes",
          "label": "Dominerende Inflammatoriske Celler",
          "type": "checkboxGroup",
          "options": [
            "Lymfocytter",
            "Plasmaceller",
            "Nøytrofile granulocytter",
            "Eosinofile granulocytter",
            "Makrofager",
            "Kjempeceller"
          ],
          "conditionalOn": "inflammationPresent",
          "conditionalValue": true,
          "generatesText": "Infiltratet består hovedsakelig av {{value}}."
        },
        {
          "id": "cryptitis",
          "label": "Kryptitt",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "inflammationPresent",
          "conditionalValue": true,
          "generatesText": "{{value ? Kryptitt observeres. : }}"
        },
        {
          "id": "cryptAbscesses",
          "label": "Kryptabscesser",
          "type": "checkbox",
          "defaultValue": false,
          "conditionalOn": "inflammationPresent",
          "conditionalValue": true,
          "generatesText": "{{value ? Kryptabscesser er til stede. : }}"
        }
      ]
    },

    {
      "id": "invasiveCarcinomaFeatures",
      "title": "Invasive Karsinomfunn",
      "fields": [
        {
          "id": "invasionSuspected",
          "label": "Mistanke om Invasiv Vekst",
          "type": "checkbox",
          "defaultValue": false,
          "generatesText": ""
        },
        {
          "id": "carcinomaDifferentiation",
          "label": "Differensieringsgrad (Karsinom)",
          "type": "dropdown",
          "options": [
            "Høygradig differensiert (Well differentiated / G1)",
            "Middelsgradig differensiert (Moderately differentiated / G2)",
            "Lavgradig differensiert (Poorly differentiated / G3)",
            "Udifferensiert (G4)"
          ],
          "defaultValue": "Middelsgradig differensiert (Moderately differentiated / G2)",
          "conditionalOn": "invasionSuspected",
          "conditionalValue": true,
          "generatesText": "Karsinomet er {{value}}."
        },
        {
          "id": "lymphovascularInvasion",
          "label": "Lymfovaskulær Invasjon (LVI)",
          "type": "dropdown",
          "options": ["Ikke påvist", "Påvist", "Ubestemt/Kan ikke vurderes"],
          "defaultValue": "Ikke påvist",
          "conditionalOn": "invasionSuspected",
          "conditionalValue": true,
          "generatesText": "Lymfovaskulær invasjon er {{value}}."
        },
        {
          "id": "perineuralInvasion",
          "label": "Perineural Invasjon (PNI)",
          "type": "dropdown",
          "options": ["Ikke påvist", "Påvist", "Ubestemt/Kan ikke vurderes"],
          "defaultValue": "Ikke påvist",
          "conditionalOn": "invasionSuspected",
          "conditionalValue": true,
          "generatesText": "Perineural invasjon er {{value}}."
        },
        {
          "id": "tumorBudding",
          "label": "Tumor Budding (ITBCC)",
          "type": "dropdown",
          "options": [
            "Bd1 (0-4 buds/lav grad)",
            "Bd2 (5-9 buds/intermediær grad)",
            "Bd3 (≥10 buds/høy grad)",
            "Kan ikke vurderes"
          ],
          "defaultValue": "Kan ikke vurderes",
          "conditionalOn": "invasionSuspected",
          "conditionalValue": true,
          "generatesText": "Tumor budding er vurdert som {{value}}."
        },
        {
          "id": "depthOfSubmucosalInvasion",
          "label": "Dybde Submukosal Infiltrasjon (pT1)",
          "type": "number",
          "unit": "µm",
          "defaultValue": 0,
          "conditionalOn": "invasionSuspected",
          "conditionalValue": true,
          "generatesText": "{{value > 0 ? Dybden av submukosal infiltrasjon er målt til + '' + value + unit + . : '' }}"
        }
      ]
    },

    {
      "id": "otherFindingsAndAncillary",
      "title": "Andre Funn og Spesialfarginger/IHC",
      "fields": [
        {
          "id": "specialStainsPerformed",
          "label": "Spesialfarginger Utført",
          "type": "text",
          "defaultValue": "",
          "generatesText": "{{value ? Følgende spesialfarginger er utført:  + value + . : }}"
        },
        {
          "id": "ihcPerformed",
          "label": "Immunhistokjemi Utført",
          "type": "text",
          "defaultValue": "",
          "generatesText": "{{value ? Immunhistokjemiske undersøkelser utført:  + value + . : }}"
        },
        {
          "id": "ihcResultsSummary",
          "label": "Sammendrag av IHC Resultater",
          "type": "text",
          "defaultValue": "",
          "conditionalOn": "ihcPerformed",
          "conditionalValue_not": "",
          "generatesText": "{{value ? Resultater:  + value + . : }}"
        },
        {
          "id": "foreignBodies",
          "label": "Fremmedlegemer",
          "type": "checkbox",
          "defaultValue": false,
          "generatesText": "{{value ? Fremmedlegemer er observert. : }}"
        },
        {
          "id": "microorganisms",
          "label": "Mikroorganismer",
          "type": "text",
          "defaultValue": "",
          "generatesText": "{{value ? Følgende mikroorganismer er observert:  + value + . : }}"
        }
      ]
    }
  ],
